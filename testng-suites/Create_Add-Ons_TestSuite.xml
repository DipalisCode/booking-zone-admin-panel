<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Create_AddOns_TestSuite" parallel="false" verbose="2">
	<parameter name="author" value="Dipali" />
	<parameter name="userType" value="business" />

	<test name="Add Add-Ons Without Tax" enabled="true">
		<classes>
			<class
				name="com.bookingzone.admin.addADDONS.tests.TC001AddAddonsWithoutTax" />
		</classes>
	</test>

	<test name="Add Add-Ons With Percentage Tax" enabled="true">
		<classes>
			<class
				name="com.bookingzone.admin.addADDONS.tests.TC002AddAddonsWithPerTax" />
		</classes>
	</test>

	<test name="Add Add-Ons With Flat Tax" enabled="true">
		<classes>
			<class
				name="com.bookingzone.admin.addADDONS.tests.TC003AddAddonsWithFlatTax" />
		</classes>
	</test>

	<test name="Add Add-Ons With Both Tax" enabled="true">
		<classes>
			<class
				name="com.bookingzone.admin.addADDONS.tests.TC004AddAddonsWithPerAndFlatTax" />
		</classes>
	</test>

</suite>
